<?xml version="1.0"?>
<definition>
  <!-- anmn_timeseries -->
  <source>
    <schema>anmn_ts</schema>
    <virtualDataTable>select ts_id as instance_id, * from measurement</virtualDataTable>
    <virtualInstanceTable>select * from timeseries</virtualInstanceTable>
  </source>
  <dimensions>
    <dimension name="TIME"/>
  </dimensions>
  <variables>
    <variable>
      <name>LATITUDE</name>
      <encoder>float</encoder>
      <attributes>
        <attribute name="_FillValue" value="99999.f"/>
      </attributes>
    </variable>
    <variable>
      <name>LONGITUDE</name>
      <encoder>double</encoder>
      <attributes>
        <attribute name="_FillValue" value="99999."/>
      </attributes>
    </variable>
    <variable>
      <name>TEMP</name>
      <encoder>float</encoder>
      <dimensions>
        <dimension name="TIME"/>
      </dimensions>
      <attributes>
        <attribute name="_FillValue" value="99999.f"/>
        <attribute name="example1" value="&quot;examplevalue&quot;" />
        <attribute name="example2" value="'examplevalue'" />
        <attribute name="example3" value="0b, 5b, 7b, 9b, 1b" />
      </attributes>
    </variable>
    <variable>
      <name>DEPTH</name>
      <encoder>float</encoder>
      <dimensions>
        <dimension name="TIME"/>
      </dimensions>
      <attributes>
        <attribute name="_FillValue" value="99999.f"/>
      </attributes>
    </variable>
    <variable>
      <name>DEPTH_quality_control</name>
      <encoder>byte</encoder>
      <dimensions>
        <dimension name="TIME"/>
      </dimensions>
      <attributes>
        <attribute name="_FillValue" value="99b"/>
      </attributes>
    </variable>
  </variables>
</definition>
